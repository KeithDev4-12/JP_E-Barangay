{$R *.dfm}

procedure TULogin.Button1Click(Sender: TObject);
begin
 If not assigned(UMainMenu) then
 UMainMenu := TUMainMenu.Create(Application);
 //ModConnection.AEmployeeID := 1;
 UMainMenu.Width := 0;
 UMainMenu.Height := 0;
 //FLogin.WindowState := wsMinimized;
